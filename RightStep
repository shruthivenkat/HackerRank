/*Observe that its base and height are both equal to , and the image is drawn using # symbols and spaces.
The last line is not preceded by any spaces.*/
//https://www.hackerrank.com/challenges/staircase/problem

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
int i;int j;
void pri(int x)
{
    for(i=0;i<x;i++)
    {
        printf("#");
    }
    printf("\n");
}
int main()
{
    int n;
    scanf("%d",&n);
    for(i=0;i<=n;i++)
    {
        pri(i);
        for(j=1;j<n-i;j++)
        {
            printf(" ");
        }
    }
}
